{
  "name": "Cute Animals Yay",
  "version": "1.0",
  "description": "Cute Animals 4 Fun ",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "browser_action":{
    "default_icon": "images/panda-face-icon.png",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "icons": {
    "16": "images/panda-face-icon.png",
    "32": "images/panda-face-icon.png",
    "48": "images/panda-face-icon.png",
    "128": "images/panda-face-icon.png"
  },
  "manifest_version": 2
}
